{% load cache %}{% spaceless %}
<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
  {% cache 86400 sitemap_locations %}
  {% for location in locations %}
  <url>
    <loc>{{ location }}</loc>
  </url>
  {% endfor %}
  {% endcache %}
</urlset>
{% endspaceless %}
